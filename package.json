{
  "name": "aws-self-mail-sender",
  "description": "A simple utility for sending an email to yourself using AWS SES, perfect for use in 'Contact Us' forms and similar scenarios. This package provides an easy-to-use function to send emails directly to your inbox, ensuring that you don't miss important messages from your users.",
  "private": false,
  "version": "1.0.6",
  "type": "commonjs",
  "keywords": [
    "AWS",
    "SES",
    "email",
    "self-mail",
    "contact-form",
    "contact-us",
    "Node.js",
    "serverless",
    "email-sender",
    "email-utility",
    "simple-email-service",
    "AWS SES",
    "email automation",
    "notification",
    "message"
  ],
  "files": [
    "dist",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.mjs"
      },
      "worker": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./fetch-aws-self-mail-aws-sender-request": {
      "import": {
        "types": "./dist/fetch-aws-self-mail-aws-sender-request/fetch-aws-self-mail-aws-sender-request.d.ts",
        "default": "./dist/fetch-aws-self-mail-aws-sender-request/fetch-aws-self-mail-aws-sender-request.esm.mjs"
      },
      "worker": {
        "types": "./dist/fetch-aws-self-mail-aws-sender-request/fetch-aws-self-mail-aws-sender-request.d.ts",
        "default": "./dist/fetch-aws-self-mail-aws-sender-request/fetch-aws-self-mail-aws-sender-request.esm.js"
      },
      "require": {
        "types": "./dist/fetch-aws-self-mail-aws-sender-request/fetch-aws-self-mail-aws-sender-request.d.ts",
        "default": "./dist/fetch-aws-self-mail-aws-sender-request/fetch-aws-self-mail-aws-sender-request.cjs.js"
      }
    },
    "./env-ses-self-mail-sender-manager": {
      "import": {
        "types": "./dist/env/env-ses-self-mail-sender-manager.d.ts",
        "default": "./dist/env/env-ses-self-mail-sender-manager.esm.mjs"
      },
      "worker": {
        "types": "./dist/env/env-ses-self-mail-sender-manager.d.ts",
        "default": "./dist/env-ses-self-mail-sender-manager.esm.js"
      },
      "require": {
        "types": "./dist/env/env-ses-self-mail-sender-manager.d.ts",
        "default": "./dist/env/env-ses-self-mail-sender-manager.cjs.js"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build:vite": "vite build",
    "build": "rimraf ./dist && npm run build:vite"
  },
  "dependencies": {
    "aws4fetch": "^1.0.20",
    "dedent": "^1.5.3",
    "fast-xml-parser": "^4.4.1"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^2.11.2",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.1",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.1.6",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.1.0"
  },
  "browser": {
    "process": "./node_modules/process/browser.js"
  }
}
